page {
  height: 100%;
}

.container {
  width: 690rpx;
  margin: 0 auto;
}

.flex {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.flex-column {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.text-center{
  text-align: center;
}

.h100 {
  height: 100%;
}

.w100 {
  width: 100%;
}

